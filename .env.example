# Logto Configuration
LOGTO_ENDPOINT=https://your-tenant.logto.app

# Machine-to-Machine Application credentials
LOGTO_M2M_APP_ID=your-m2m-app-id
LOGTO_M2M_APP_SECRET=your-m2m-app-secret

# Webhook Configuration (optional)
LOGTO_WEBHOOK_SECRET=your-webhook-signing-key

# API Key for auth and webhook endpoints (optional but recommended)
API_KEY=your-api-key

# Server Configuration
PORT=3000

# Logging
LOG_LEVEL=info  # debug, info, warn, error
